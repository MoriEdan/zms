{
	"db_name": "zms",
	"db_file": "zms.db",
	"version": 2,
	"maps": {
		"content.json": {
			"to_table": []
		},
		"data/authors/content.json": {
			"to_table": []
		},
		"data/(users|authors)/.+/content.json": {
			"to_json_table": ["cert_user_id"],
			"file_name": "data.json"
		},
		"data/users/.+/data.json": {
			"to_table": []
		},
		"data/authors/.+/data.json": {
			"to_table": ["posts"]
		}
	},
	"tables": {
		"json": {
			"cols": [
				["json_id", "INTEGER PRIMARY KEY AUTOINCREMENT"],
				["directory", "TEXT"],
				["file_name", "TEXT"],
				["cert_user_id", "TEXT"]
			],
			"indexes": ["CREATE UNIQUE INDEX path ON json(directory, file_name)"],
			"schema_changed": 5
		},
		"posts": {
			"cols": [
				["id", "INTEGER"],
				["title", "TEXT"],
				["content", "TEXT"],
				["cut", "TEXT"],
				["date", "INTEGER"],
				["json_id", "INTEGER"]
			],
			"indexes": [],
			"schema_changed": 2
		}
	},
	"zms_hotreloads": {
		"post": [],
		"layout": []
	}
}